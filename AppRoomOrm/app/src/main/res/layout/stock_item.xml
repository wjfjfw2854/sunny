<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="item"
            type="cn.wjf.approomorm.db.stockdb.StockList" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="80dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/goodid"
            android:textSize="8sp"
            android:textColor="#ff83"
            android:paddingLeft="2dp"
            android:text="@{String.valueOf(item.goodid)}"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/name"
            app:layout_constraintBottom_toBottomOf="parent"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/name"
            android:textSize="6sp"
            android:textColor="#ff83"
            android:paddingLeft="2dp"
            android:text="@{String.valueOf(item.name)}"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/goodid"
            app:layout_constraintRight_toLeftOf="@+id/code"
            app:layout_constraintBottom_toBottomOf="parent"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/code"
            android:textSize="6sp"
            android:textColor="#ff83"
            android:paddingLeft="2dp"
            android:text="@{String.valueOf(item.code)}"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/name"
            app:layout_constraintRight_toLeftOf="@+id/pinyin"
            app:layout_constraintBottom_toBottomOf="parent"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/pinyin"
            android:textSize="6sp"
            android:textColor="#ff83"
            android:paddingLeft="2dp"
            android:text="@{String.valueOf(item.pinyin)}"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/code"
            app:layout_constraintRight_toLeftOf="@+id/exchange"
            app:layout_constraintBottom_toBottomOf="parent"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/exchange"
            android:textSize="6sp"
            android:textColor="#ff83"
            android:paddingLeft="2dp"
            android:text="@{String.valueOf(item.exchange)}"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/pinyin"
            app:layout_constraintRight_toLeftOf="@+id/category"
            app:layout_constraintBottom_toBottomOf="parent"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/category"
            android:textSize="6sp"
            android:textColor="#ff83"
            android:paddingLeft="2dp"
            android:text="@{String.valueOf(item.category)}"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/exchange"
            app:layout_constraintRight_toLeftOf="@+id/trade_session"
            app:layout_constraintBottom_toBottomOf="parent"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/trade_session"
            android:textSize="6sp"
            android:textColor="#ff83"
            android:paddingLeft="2dp"
            android:text="@{String.valueOf(item.trade_session)}"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/category"
            app:layout_constraintRight_toLeftOf="@+id/version"
            app:layout_constraintBottom_toBottomOf="parent"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/version"
            android:textSize="6sp"
            android:textColor="#ff83"
            android:paddingLeft="2dp"
            android:text="@{String.valueOf(item.version)}"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/trade_session"
            app:layout_constraintRight_toLeftOf="@+id/weight"
            app:layout_constraintBottom_toBottomOf="parent"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/weight"
            android:textSize="6sp"
            android:textColor="#ff83"
            android:paddingLeft="2dp"
            android:text="@{String.valueOf(item.weight)}"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/version"
            app:layout_constraintRight_toLeftOf="@+id/format_name"
            app:layout_constraintBottom_toBottomOf="parent"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:id="@+id/format_name"
            android:textSize="6sp"
            android:textColor="#ff83"
            android:paddingLeft="2dp"
            android:text="@{String.valueOf(item.format_name)}"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/weight"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>